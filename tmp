$path = 'C:\Program Files (x86)'
# Delete -WhatIf to actually delete folders, with this switch it will only output what is it going to do
Get-ChildItem $path -Force -Directory -Filter chrome_BITS_* | Remove-Item -Recurse -Confirm:$false -Force
Get-ChildItem $path -Force -Directory -Filter chrome_ComponentUnpacker_BeginUnzipping* | Remove-Item -Recurse -Confirm:$false -Force
Get-ChildItem $path -Force -Directory -Filter chrome_url_fetcher_* | Remove-Item -Recurse -Confirm:$false -Force
Get-ChildItem $path -Force -Directory -Filter scoped_dir** | Remove-Item -Recurse -Confirm:$false -Force
